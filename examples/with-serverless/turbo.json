{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "sls:package:dev": {
      "inputs": ["src/**"],
      "outputs": [".serverless/**"]
    },
    "sls:package:prod": {
      "inputs": ["src/**"],
      "outputs": [".serverless/**"]
    },
    "sls:deploy:dev": {
      "inputs": [".serverless/**"],
      "outputs": [".serverless/**"]
    },
    "sls:deploy:prod": {
      "inputs": [".serverless/**"],
      "outputs": [".serverless/**"]
    }
  }
}
